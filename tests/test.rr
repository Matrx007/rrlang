static testNum = 21, testDivider = 4: u32;

fn div(a: u32, divider: u32): u32 {
    if(divider == 0) {
        return 0;
    };

    count = 0: u32;

    while(a > divider) {
        a = a - divider;
        count++;
    };

    return count;
};

fn dot(a: [u32, u32, u32], b: [u32, u32, u32]): u32 {
    return a[0] * b[0] + a[1] * b[1] + a[2] * b[2];
};

fn main(): void {
    div(testNum, testDivider);
};
